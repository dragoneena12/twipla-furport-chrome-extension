{
  "name": "Twipla-Furport chrome extension",
  "version": "0.1.0",
  "description": "Twipla-Furport chrome extension",
  "permissions": ["activeTab", "declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "images/get_started32.png"
    }
  },
  "icons": {
    "48": "images/get_started48.png"
  },
  "manifest_version": 2
}
